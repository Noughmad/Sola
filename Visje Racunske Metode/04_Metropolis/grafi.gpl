set terminal epslatex color solid

set logscale xy

set output "g_plot_energy.tex"
plot for [l in "0 0.3 1 3"] sprintf("g_energija_%s.dat",l) u 1:4 t sprintf("$\\lambda = %s$$", l)

set output "g_plot_x.tex"
plot for [l in "0 0.3 1 3"] sprintf("g_energija_%s.dat",l) u 1:6 t sprintf("$\\lambda = %s$$", l)
